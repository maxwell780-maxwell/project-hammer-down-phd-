$modelname "deadlock/candle_gaurd.mdl"       // output model path

$cdmaterials "models\candle_gaurd" // folder with VMTs

// Body mesh
$body "Body" "super_trooper_humanoid_ALL"

$model "candle_gaurd" "super_trooper_humanoid_ALL.smd" {

 flexfile "models_npc_trooper_super_trooper.vta" 
 {
  defaultflex frame 0
  flex "mid" frame 1
  flex "low" frame 2
 }
 
 flexcontroller candle_gaurd_health_mid range 0 1 "mid"
 flexcontroller candle_gaurd_health_low range 0 1 "low"

 %mid = mid
 %low = low 
} 


$contents "solid"

$illumposition 0 6.335 39.76

// ADDED DEFINE BONE NOW CROWBAR WILL NOT COMPLAIN WHICH CROWBAR DID SO F*CK CROWBAR IMMA REMOVE SOME OF THE BONES THAT CROWBAR IS LOOPING OVER

